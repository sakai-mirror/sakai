<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

  <class
    name="org.sakaiproject.component.common.superstructure.NodeParentChildMapImpl"
    table="CMN_NODE_PARENT_CHILD_MAP_T" optimistic-lock="version">

    <cache usage="read-write" />

    <id name="id">
      <column name="ID" not-null="true" length="19" />
      <generator class="native">
        <param name="sequence">CMN_NODE_PARENT_CHILD_MAP_S</param>
      </generator>
    </id>

    <version name="version" column="VERSION" />

    <many-to-one name="parent" cascade="none"
      class="org.sakaiproject.component.common.superstructure.NodeImpl">
      <column name="PARENT" index="CMN_NODE_PCMAP_PARENT_I"
        not-null="true" unique-key="CMN_NODE_PCMAP_UNIQUE_I"
        unique="false" />
    </many-to-one>

    <many-to-one name="child" cascade="none"
      class="org.sakaiproject.component.common.superstructure.NodeImpl">
      <column name="CHILD" index="CMN_NODE_PCMAP_CHILD_I"
        not-null="true" unique-key="CMN_NODE_PCMAP_UNIQUE_I"
        unique="false" />
    </many-to-one>

  </class>

</hibernate-mapping>
