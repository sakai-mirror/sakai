<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

<!--  Register Our IdManager  -->
        <bean id="org.osid.id.IdManager"
                class="org.sakaiproject.component.osid.id.IdManager"
                singleton="true">
                <!-- No injections yet -->
        </bean>

<!--  Register Our LogManager  -->
        <bean id="org.osid.logging.LoggingManager"
                class="org.sakaiproject.component.osid.logging.LoggingManager"
                singleton="true">
                <!-- No injections yet -->
        </bean>

<!--  Register Our RepositoryManager  -->
        <bean id="org.osid.repository.RepositoryManager"
                class="org.sakaiproject.component.osid.repository.registry.RepositoryManager"
                singleton="true">
                <!-- No injections yet -->
        </bean>

</beans>
