<div class="portletBody">
#if($menu)
	#toolbar($menu)
#end
	
		#if ($alertMessage)<div class="alertMessage">$tlang.getString("useconrem.alert") $validator.escapeHtml($alertMessage)</div>#end
		<div class="alertMessage">
			$tlang.getString("useconrem.areyousur")
		</div>
		<table class="listHier" cellspacing="0" summary ="List of Users">
			<tr>
				<th id="Id">$tlang.getString("useconrem.useid")</th>
