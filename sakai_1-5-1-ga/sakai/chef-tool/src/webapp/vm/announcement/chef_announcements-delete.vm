<!-- $Header: /cvs/sakai/chef-tool/src/webapp/vm/announcement/chef_announcements-delete.vm,v 1.2 2004/07/26 19:56:00 suiyy.umich.edu Exp $ --><div class="chefPortletContainer">	<div class ="chefPortletToolBarMessage">		Deleting announcements...	</div>	<div class="chefPortletContent">		#if ($alertMessage)<div class="chefAlertBox">Alert: $validator.escapeHtml($alertMessage)</div>#end	<div class="chefAlertBox">Are you sure you want to delete the following announcements?</div>		<form name="forNotLetDeleteSelectedOnLoad"></form>			<table class="chefFlatListViewTable" cellspacing="0">				<tr>					<th>Subject</th>					<th>From</th>					<th>Date</th>				</tr>				#foreach ($delete_item in $delete_messages)					<tr>						<td>							#if ($delete_item.Header.draft)								<span class="chefAlert">Draft - </span>							#end							$validator.escapeHtml($delete_item.Header.subject)						</td>						<td>$validator.escapeHtml($delete_item.Header.From.DisplayName)</td>						<td>$delete_item.Header.Date.toStringLocalFull()</td>					</tr>				#end			</table>		<form name="deleteTemplateForm" action="#toolForm("AnnouncementAction")" method="post">			<div class="chefButtonRow">				<input type="submit" name="eventSubmit_doDelete" value="Delete" />				<input type="submit" value="Cancel" name="eventSubmit_doCancel" />			</div>		</form>	</div></div>