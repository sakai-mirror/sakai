##<!-- $Header: /cvs/sakai/chef-tool/src/webapp/vm/admin/chef_presence.sessions-List.vm,v 1.6 2004/09/30 20:21:41 ggolden.umich.edu Exp $ -->
<div class="chefPortletContainer">
#if($menu)
	#toolbar($menu)
#end
	<div class ="chefPortletToolBarMessage">
		Active Sessions
	</div>
	<div class="chefPortletContent">
		#if ($alertMessage)<div class="chefAlertBox">Alert: $validator.escapeHtml($alertMessage)</div>#end
		<div>Total sessions: $total</div>
		<table class ="chefFlatListViewTable" cellspacing="0" summary ="Table holds users present">	
#foreach ($serverName in $serverList)
		<tr>
			<th colspan="3">$servers.get("$serverName").size() on: $validator.escapeHtml($serverName)</th>
		</tr>
	#foreach ($session in $servers.get("$serverName"))
		<tr>
			<td rowspan="5" class="highLight" >$velocityCount</td>
			<td class="chefTopline">Session Id:</td>
			<td class="chefTopline">$validator.escapeHtml($session.Id)</td>
		</tr>
		<tr>
			<td nowrap="nowrap">Name(UserId):</td>
			<td>$validator.escapeHtml($session.UserId)</td>
		</tr>
		<tr>
			<td>Ip Address:</td>
			<td>$validator.escapeHtml($session.IpAddress)</td>															
		</tr>
		<tr>
			<td>Agent Type:</td>
			<td>$validator.escapeHtml($session.UserAgent)</td>															
		</tr>
		<tr>
			<td>Started:</td>
			<td>$validator.escapeHtml($session.getStart().toStringLocalFull())</td>															
		</tr>	
	#end
#end
		</table>
	</div>
</div>
