<beans>
   <bean id="worksiteOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>apkr</value></property>
      <property name="name"><value>Worksite Information Overview</value></property>
      <property name="location"><value>/sakai_worksite/apkr.html</value></property>
      <property name="defaultForTool"><value>sakai.worksite</value></property>
   </bean>
    
       <bean id="changeWorksiteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>apne</value></property>
      <property name="name"><value>Change size of Worksite Information Box</value></property>
      <property name="location"><value>/sakai_worksite/apne.html</value></property>
   </bean>

    <bean id="addWorksiteInfo" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>apnd</value></property>
      <property name="name"><value>Add/modify content in Worksite Information Box</value></property>
      <property name="location"><value>/sakai_worksite/apnd.html</value></property>
   </bean>
   
