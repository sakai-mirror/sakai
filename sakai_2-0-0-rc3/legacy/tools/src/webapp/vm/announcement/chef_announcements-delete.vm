<!-- $Header: /cvs/sakai2/legacy/tools/src/webapp/vm/announcement/chef_announcements-delete.vm,v 1.2 2005/05/15 04:54:19 gsilver.umich.edu Exp $ --><div class="portletBody">	#if ($alertMessage)<div class="chefAlertBox">$tlang.getString("gen.alert") $validator.escapeHtml($alertMessage)</div>#end	<h3>		$tlang.getString("del.deleting")	</h3>	<div class="chefAlertBox">$tlang.getString("del.areyou")</div>	<form name="forNotLetDeleteSelectedOnLoad"></form>		<table class="listHier" cellspacing="0">			<tr>				<th>$tlang.getString("gen.subject")</th>				<th>$tlang.getString("gen.from")</th>				<th>$tlang.getString("gen.date")</th>			</tr>			#foreach ($delete_item in $delete_messages)				<tr>					<td>						#if ($delete_item.Header.draft)							<span class="chefAlert">$tlang.getString("gen.draft") - </span>						#end						$validator.escapeHtml($delete_item.Header.subject)					</td>					<td>$validator.escapeHtml($delete_item.Header.From.DisplayName)</td>					<td>$delete_item.Header.Date.toStringLocalFull()</td>				</tr>			#end		</table>	<form name="deleteTemplateForm" action="#toolForm("AnnouncementAction")" method="post">		<p class="act">			<input type="submit" class="active" name="eventSubmit_doDelete" value="$tlang.getString("gen.delete")" />			<input type="submit" value="$tlang.getString("gen.cancel")" name="eventSubmit_doCancel" />		</p>	</form></div>