<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="membershipOverview"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqjo</value>
		</property>
		<property name="name">
			<value>Membership Overview</value>
		</property>
		<property name="location">
			<value>/sakai_membership/aqjo.html</value>
		</property>
		<property name="defaultForTool">
			<value>sakai.membership</value>
		</property>
	</bean>

	<bean id="org.sakaiproject.api.app.help.TableOfContents"
		class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
		<property name="name">
			<value>root</value>
		</property>
		<property name="categories">
			<list>
				<bean id="membershipCategory"
					class="org.sakaiproject.component.app.help.model.CategoryBean">
					<property name="name">
						<value>Membership</value>
					</property>
					<property name="resources">
						<list>
							<ref bean="membershipOverview" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>
