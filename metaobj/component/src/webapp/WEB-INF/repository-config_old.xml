         <ref bean="org.sakaiproject.metaobj.repository.intf.NodeTreeService"/>
      </property>
      <property name="nodeMetadataService">
         <ref bean="org.sakaiproject.metaobj.repository.intf.NodeMetadataService"/>
      </property>
   </bean>

