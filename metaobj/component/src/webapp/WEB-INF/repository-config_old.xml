   <bean id="org.sakaiproject.metaobj.repository.mgt.impl.WckRepositoryManagerHelper"
      class="org.sakaiproject.metaobj.repository.mgt.impl.WckRepositoryManagerHelper">
      <property name="wck">
         <ref bean="wck"/>
      </property>
      <property name="authManager">
         <ref bean="authManager"/>
      </property>
      <property name="agentManager">
         <ref bean="agentManager"/>
      </property>
      <property name="homeFactory">
         <ref bean="homeFactory"/>
      </property>
   </bean>

   <bean id="repositoryManager" class="org.sakaiproject.metaobj.repository.mgt.impl.WckRepositoryManager">
      <property name="wck">
         <ref bean="wck"/>
      </property>
      <property name="quotaDao">
         <ref bean="quotaDao"/>
      </property>
      <property name="authManager">
         <ref bean="authManager"/>
      </property>
      <property name="agentManager">
         <ref bean="agentManager"/>
      </property>
      <property name="idManager">
         <ref bean="idManager"/>
      </property>
      <property name="authzManager">
         <ref bean="authzManager"/>
      </property>
      <property name="baseDir">
         <value>/files</value>
      </property>
      <!--<property name="slideUserBase"><value>/slide/users/</value></property>-->
      <property name="host">
         <value>${system.repository.host}</value>
      </property>
      <property name="userRootNodeType">
         <bean class="org.sakaiproject.metaobj.shared.model.Type">
            <property name="description">
               <value>User Root Node</value>
            </property>
            <property name="systemOnly">
               <value>true</value>
            </property>
         </bean>
      </property>
      <property name="wckRepositoryManagerHelper">
         <ref bean="org.sakaiproject.metaobj.repository.mgt.impl.WckRepositoryManagerHelper"/>
      </property>
      <property name="worksiteManager">
         <ref bean="org.sakaiproject.metaobj.worksite.mgt.WorksiteManager"/>
      </property>
      <property name="mimeTypeManager">
         <ref bean="org.sakaiproject.metaobj.repository.mgt.intf.MimeTypeManager"/>
      </property>
   </bean>
   <bean id="nodeType" class="org.sakaiproject.metaobj.shared.model.Type">
      <property name="description">
         <value>Node</value>
      </property>
      <property name="systemOnly">
         <value>true</value>
      </property>
   </bean>
   <bean id="fileValidator" class="org.sakaiproject.metaobj.repository.control.FileNameValidator">
      <!--<property name="fileNamePattern"><value>[^\'\"\:\;\[\]\{\}\~\`\!\@\#\$\%\^\&amp;\*\(\)\+\/\?\&lt;\&gt;\,\|\\]*</value></property>-->
      <property name="fileNamePattern">
         <value>[\w \-\.]*</value>
      </property>
   </bean>

   <bean id="quotaDao" class="org.sakaiproject.metaobj.repository.model.impl.QuotaDaoImpl">
      <property name="sessionFactory">
         <ref bean="sessionFactory"/>
      </property>
      <property name="defaultQuota">
         <ref bean="defaultQuota"/>
      </property>
      <property name="nodeMetadataService">
         <ref bean="org.sakaiproject.metaobj.repository.intf.NodeMetadataService"/>
      </property>
   </bean>

   <bean id="defaultQuota" class="java.lang.Long">
      <constructor-arg index="0" type="java.lang.String">
         <description>Maximum number of bytes a user may store</description>
         <value>20000000</value>
      </constructor-arg>
   </bean>


   <bean id="fileArtifactFinder" class="org.sakaiproject.metaobj.repository.mgt.impl.FileArtifactFinderImpl">
      <property name="homeFactory">
         <ref bean="homeFactory"/>
      </property>
