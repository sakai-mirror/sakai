<beans default-lazy-init="true">

   <bean id="org.sakaiproject.metaobj.repository.intf.NodeTreeService"
      class="org.sakaiproject.metaobj.repository.impl.NodeTreeServiceImpl">
      <property name="sessionFactory">
         <ref bean="sessionFactory"/>
      </property>
   </bean>

   <bean id="org.sakaiproject.metaobj.repository.intf.NodeMetadataService"
      class="org.sakaiproject.metaobj.repository.impl.NodeMetadataServiceImpl">
      <property name="authenticationManager">
         <ref bean="authManager"/>
      </property>
      <property name="sessionFactory">
         <ref bean="sessionFactory"/>
      </property>
      <property name="nodeTreeService">
         <ref bean="org.sakaiproject.metaobj.repository.intf.NodeTreeService"/>
      </property>
   </bean>

   <bean id="org.sakaiproject.metaobj.repository.intf.StreamStore"
      class="org.sakaiproject.metaobj.repository.impl.StreamStoreImpl"
      init-method="init">
      <property name="sessionFactory">
         <ref bean="sessionFactory"/>
      </property>
      <property name="repositoryPath">
         <value>${repository.path}</value>
      </property>
      <property name="idManager">
         <ref bean="idManager"/>
      </property>
   </bean>

   <bean id="org.apache.commons.pool.ObjectPool.slideReferenceBasicFileStore"
      class="org.sakaiproject.metaobj.repository.mgt.impl.pool.ProtectedStorePool">
      <property name="factory">
         <bean class="org.sakaiproject.metaobj.repository.mgt.impl.pool.ProtectedStorePoolFactory">
            <property name="protectedStoreName">
               <value>wck</value>
            </property>
         </bean>
      </property>
   </bean>

   <bean id="SlideReferenceBasicFileStore" class="org.apache.slide.simple.reference.WebdavFileStore"
      singleton="false">
   </bean>

   <bean id="lockManager" class="org.sakaiproject.metaobj.repository.mgt.impl.LockManagerImpl">
      <property name="sessionFactory">
         <ref bean="sessionFactory"/>
      </property>
   </bean>

   <bean id="org.sakaiproject.metaobj.repository.mgt.webdav.WckProtectionProxyConfig"
      class="org.sakaiproject.metaobj.repository.mgt.webdav.WckProtectionProxyConfig"
      singleton="false">
      <property name="transactionManager">
         <ref bean="transactionManager"/>
      </property>
      <property name="rootNodeName">
         <value>files</value>
      </property>
      <property name="fileNameValidator">
         <ref bean="fileValidator"/>
      </property>
