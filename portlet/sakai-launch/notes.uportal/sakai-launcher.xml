<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition SYSTEM "channelDefinition.dtd">

<!-- You can download this Google Sample Portlet from the Portlet Open Source Trading site (POST).
     http://portlet-opensrc.sourceforge.net/
     http://prdownloads.sourceforge.net/portlet-opensrc/GooglePortlet.zip?download
-->
    
<channel-definition>

    <title>Sakai Launcher Portlet</title>
    <name>Sakai Launcher Portlet</name>
    <fname>sakai-launcher-portlet</fname>
    <desc>The Sakai Launcher Portlet</desc>
    <type>Portlet</type>
    <class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
    <timeout>10000</timeout>
    
    <hasedit>Y</hasedit>
    <hashelp>Y</hashelp>
    <hasabout>N</hasabout>

    <secure>N</secure>
    <locale>en_US</locale>
    
    <categories>
        <category>Applications</category>
    </categories>
    
    <groups>
        <group>Everyone</group>
    </groups>
    
    <parameters>

        <!-- The syntax of the ID is [portlet-context-name].[portlet-name] -->
        <parameter>
            <name>portletDefinitionId</name>
            <value>sakai-test-portlets.SakaiLaunch</value>
            <description>Identifies the portlet deployed within the portlet container</description>
            <ovrd>N</ovrd>
        </parameter>

    </parameters>
    
</channel-definition>

