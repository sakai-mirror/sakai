<div class="chefPortletContainer">
#if($menu)
	#toolbar($menu)
#end
	<div class ="chefPortletToolBarMessage">
		Active Servers
	</div>
	<div class="chefPortletContent">
		#if ($alertMessage)<div class="chefAlertBox">Alert: $validator.escapeHtml($alertMessage)</div>#end
		<div>Total sessions: $total</div>
		<table class ="chefFlatListViewTable" cellspacing="0" summary ="Table holds servers active">
			<tr>
				<th id="Id">Server Id</th>
				<th id="Count">Sessions</th>
			</tr>
		#foreach ($serverName in $serverList)
			<tr>
				<td headers="Id">
					$validator.escapeHtml($serverName)
				</td>
				<td headers="Count">
					$servers.get("$serverName").size()
				</td>
			</tr>
		#end
		</table>
	</div>
</div>
