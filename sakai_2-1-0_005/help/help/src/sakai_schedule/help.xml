<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="scheduleOverview"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aray</value>
		</property>
		<property name="name">
			<value>Schedule Overview</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aray.html</value>
		</property>
		<property name="defaultForTool">
			<value>sakai.schedule</value>
		</property>
	</bean>
	<bean id="addActivity"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyn</value>
		</property>
		<property name="name">
			<value>Add Activity</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyn.html</value>
		</property>
	</bean>
	<bean id="addActivityAttachment"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argn</value>
		</property>
		<property name="name">
			<value>Add Attachment to Activity</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argn.html</value>
		</property>
	</bean>
	<bean id="viewSchedule"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzh</value>
		</property>
		<property name="name">
			<value>View Activity Detail</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzh.html</value>
		</property>
	</bean>
	<bean id="deleteActivity"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqzc</value>
		</property>
		<property name="name">
			<value>Edit/Delete Activity</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqzc.html</value>
		</property>
	</bean>

	<bean id="changeScheduleView"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyp</value>
		</property>
		<property name="name">
			<value>Change Schedule View</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyp.html</value>
		</property>
	</bean>

	<bean id="printCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>arfe</value>
		</property>
		<property name="name">
			<value>Add field to calendar</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/arfe.html</value>
		</property>
	</bean>

	<bean id="mergeCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>aqyl</value>
		</property>
		<property name="name">
			<value>Merge Calendars</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/aqyl.html</value>
		</property>
	</bean>
	
	<bean id="importCalendar"
		class="org.sakaiproject.component.app.help.model.ResourceBean">
		<property name="docId">
			<value>argo</value>
		</property>
		<property name="name">
			<value>Import Calendars</value>
		</property>
		<property name="location">
			<value>/sakai_schedule/argo.html</value>
		</property>
	</bean>


	<bean id="org.sakaiproject.api.app.help.TableOfContents"
		class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
		<property name="name">
			<value>root</value>
		</property>
		<property name="categories">
			<list>
				<bean id="scheduleCategory"
					class="org.sakaiproject.component.app.help.model.CategoryBean">
					<property name="name">
						<value>Schedules</value>
					</property>
					<property name="resources">
						<list>
							<ref bean="scheduleOverview" />
							<ref bean="addActivity" />
							<ref bean="addActivityAttachment" />
							<ref bean="viewSchedule" />
							<ref bean="deleteActivity" />
							<ref bean="changeScheduleView" />
							<ref bean="printCalendar" />
							<ref bean="mergeCalendar" />
							<ref bean="importCalendar" />
						</list>
					</property>
				</bean>
			</list>
		</property>
	</bean>
</beans>
