<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="gradebookOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>gradebook_overview</value>
        </property>
        <property name="name">
            <value>Gradebook Overview</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/arap.html</value>
        </property>
		<property name="defaultForTool"><value>sakai.gradebook</value></property>
    </bean>
    <bean id="gradebookAddAssignment" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>aran</value>
        </property>
        <property name="name">
            <value>Creating/adding, editing, or deleting a Gradebook assignment</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/aran.html</value>
        </property>
    </bean>
    <bean id="gradebookAssignmentDetails" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>aras</value>
        </property>
        <property name="name">
            <value>Viewing assignment details</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/aras.html</value>
        </property>
    </bean>
    <bean id="gradebookEnteringScores" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>araj</value>
        </property>
        <property name="name">
            <value>Entering or editing Gradebook grades</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/araj.html</value>
        </property>
    </bean>
    <bean id="gradebookSorting" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>arby</value>
        </property>
        <property name="name">
            <value>Sorting Gradebook tables</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/arby.html</value>
        </property>
    </bean>
    <bean id="gradebookFeedbackOptions" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>arah</value>
        </property>
        <property name="name">
            <value>Setting student feedback options</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/arah.html</value>
        </property>
    </bean>
    <bean id="gradebookRoster" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>aqzg</value>
        </property>
        <property name="name">
            <value>Viewing and exporting student roster from the Gradebook</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/aqzg.html</value>
        </property>
    </bean>
    <bean id="gradebookExportScores" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>aqzt</value>
        </property>
        <property name="name">
            <value>Exporting scores from the Gradebook</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/aqzt.html</value>
        </property>
    </bean>
    <bean id="gradebookCourseGradeOverride" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>arbv</value>
        </property>
        <property name="name">
            <value>Overriding an autocalculated course grade</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/arbv.html</value>
        </property>
    </bean>
    <bean id="gradebookCumulativeScores" class="org.sakaiproject.component.app.help.model.ResourceBean">
        <property name="docId">
            <value>asbw</value>
        </property>
        <property name="name">
            <value>Viewing and interpreting cumulative scores</value>
        </property>
        <property name="location">
            <value>/sakai_gradebook_tool/asbw.html</value>
        </property>
    </bean>
    <bean id="org.sakaiproject.api.app.help.TableOfContents" class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
        <property name="name">
            <value>root</value>
        </property>
        <property name="categories">
            <list>
                <bean id="gradebookCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
                    <property name="name">
                        <value>Gradebook Tool</value>
                    </property>
                    <property name="resources">
                        <list>
                            <ref bean="gradebookOverview"/>
                            <ref bean="gradebookAddAssignment"/>
                            <ref bean="gradebookAssignmentDetails"/>
                            <ref bean="gradebookEnteringScores"/>
                            <ref bean="gradebookSorting"/>
                            <ref bean="gradebookRoster"/>
                            <ref bean="gradebookCumulativeScores"/>
                            <ref bean="gradebookCourseGradeOverride"/>
                            <ref bean="gradebookFeedbackOptions"/>
                            <ref bean="gradebookExportScores"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
</beans>
