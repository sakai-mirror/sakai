<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
        version="1.0">

    <portlet>
        <portlet-name>SakaiGallery</portlet-name>
        <display-name xml:lang="en">Sakai Gallery Portlet</display-name>
        <description xml:lang="en">
		This portlet shows a Sakai site minus the top login 
		banner.  This portlet is used to effectively include "all of 
	 	Sakai" in a JSR-168 portal.
	</description>
        <portlet-class>org.sakaiproject.portlets.SakaiLaunch</portlet-class>
        <portlet-info>
            <title>Sakai Portlet</title>
            <short-title>Sakai Portlet</short-title>
            <keywords>Sakai</keywords>
        </portlet-info>

       <init-param>
            <name>sakai.tool</name>
            <value>gallery</value>
       </init-param>

	<!-- These sakai.host and sakai.secret properties can be set 
	     here or in sakaiportlet.properties

             If they are set here, they override the values in the properties
	     file.  This allows override on a portlet by portlet basis.

           <init-param>
                <name>sakai.host</name>
                <value>http://nightly2.sakaiproject.org:8083</value>
           </init-param>
           <init-param>
                <name>sakai.secret</name>
                <value>secret-from-init-parms</value>
           </init-param>
	-->

        <expiration-cache>60</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>

    </portlet>

    <portlet>
        <portlet-name>SakaiLaunch</portlet-name>
        <display-name xml:lang="en">Sakai Tree Portlet</display-name>
        <description xml:lang="en">
		This portlet retrieves Sakai sites, pages, and tools via web services
		and displays the tool hieratchy in a tree control on the left hand side 
		of the portlet.  The user can switch betweem Sakai pages and tools 
		using the tree control.  The tree control replaces the site and page
		navigation from Sakai.
	</description>
        <portlet-class>org.sakaiproject.portlets.SakaiLaunch</portlet-class>
        <portlet-info>
            <title>Sakai Portlet</title>
            <short-title>Sakai Portlet</short-title>
            <keywords>Sakai</keywords>
        </portlet-info>
        <description xml:lang="en">Sakai Launcher</description>

       <init-param>
            <name>sakai.tool</name>
            <value>tree</value>
       </init-param>

        <expiration-cache>60</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>edit</portlet-mode>
            <portlet-mode>help</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
    </portlet>


