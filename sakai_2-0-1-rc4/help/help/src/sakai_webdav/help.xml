<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
   <bean id="webDavOverview" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqyy</value></property>
      <property name="name"><value>WebDAV Overview</value></property>
      <property name="location"><value>/sakai_webdav/aqyy.html</value></property>
      <property name="defaultForTool"><value>sakai.webdav</value></property>
   </bean>
   
   <bean id="winxpAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>araf</value></property>
      <property name="name"><value>Access WebDAV in Windows XP</value></property>
      <property name="location"><value>/sakai_webdav/araf.html</value></property>
   </bean>
   
   <bean id="macAccess" class="org.sakaiproject.component.app.help.model.ResourceBean">
      <property name="docId"><value>aqyi</value></property>
      <property name="name"><value>Access WebDAV in Mac OS X</value></property>
      <property name="location"><value>/sakai_webdav/aqyi.html</value></property>
   </bean>
   
   
   
    <bean id="org.sakaiproject.api.app.help.TableOfContents" 
          class="org.sakaiproject.component.app.help.model.TableOfContentsBean">
      <property name="name"><value>root</value></property>
      <property name="categories">
         <list>
            <bean id="webDavCategory" class="org.sakaiproject.component.app.help.model.CategoryBean">
               <property name="name"><value>WebDAV</value></property>
               <property name="resources">
                  <list>
                     <ref bean="webDavOverview"/>
                     <ref bean="winxpAccess"/>
                     <ref bean="macAccess"/>
                  </list>
               </property>
            </bean>
           </list>
         </property>
       </bean>
</beans>
