<?xml version="1.0" encoding="UTF-8"?>
<project default="sakai:build"
	xmlns:j="jelly:core"
	xmlns:ant="jelly:ant"
	xmlns:maven="jelly:maven"
	xmlns:u="jelly:util">

	<goal name="javadoc">
		<j:set var="goal" value="javadoc" />
		<attainGoal name="multiproject:goal" />
	</goal>

	<goal name="load-full">
		<j:set var="goal" value="load-full" />
		<j:set var="maven.multiproject.includes" value="tool/project.xml" />
		<attainGoal name="multiproject:goal" />
	</goal>

	<goal name="schema">
		<j:set var="goal" value="schema" />
		<j:set var="maven.multiproject.includes" value="tool/project.xml" />
		<attainGoal name="multiproject:goal" />
	</goal>

</project>
